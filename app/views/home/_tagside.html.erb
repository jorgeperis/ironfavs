<div class="delete-tag optionstags">
  DELETE
</div>
<div class="tagscontainer-side">
  <% @usertags.each do |user_tag| %>
    <div class="tags tag-color-<%= Color.find(user_tag.tag_color).name %> size<%= user_tag.relative_size_for_user(@max_websites_tag) %>" data-user-tag="<%= user_tag.id %>" draggable="true"
    ondragstart="evdragstart(event,this)">
      <%= user_tag.tag.name %>
    </div>
  <% end %>
</div>
